# /etc/systemd/system/start_day.service

[Unit]
Description=launch start of day journal
Wants=start_day.timer
PartOf=graphical-session.target

[Service]
Type=forking
WorkingDirectory=%h/Workspace/Writing/writing_tools
ExecStart=%h/Workspace/Writing/writing_tools/Journal_Launcher start_day.md

[Install]
WantedBy=default.target
